<%- include('../header'); %>
		<main class="container-fluid main-login">
			<div class="row justify-content-center">
				<div class="col-4">
					<form class="form-login" method="POST" action="/panel/login">
						  	<div class="form-group">
						    	<label for="exampleInputEmail1">Login</label>
						    	<input type="login" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" name="login" placeholder="Enter login">
						  	</div>
						  	<div class="form-group">
						   		<label for="exampleInputPassword1">Password</label>
						    	<input type="password" class="form-control" id="exampleInputPassword1" placeholder="Password" name="password">
						  	</div>
						  	<button type="submit" class="btn btn-primary">Log in</button>
					</form>
				</div>
			</div>
		</main>
	</body>
</html>